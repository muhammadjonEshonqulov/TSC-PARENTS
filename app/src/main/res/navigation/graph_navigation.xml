<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/graph_nav"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="uz.jbnuu.tsc.parents.ui.login.LoginFragment"
        android:label="AuthFragment"
        tools:layout="@layout/login_fragment">

        <action
            android:id="@+id/action_loginFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/graph_nav"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />

    </fragment>


    <fragment
        android:id="@+id/studentMainFragment"
        android:name="uz.jbnuu.tsc.parents.ui.student_main.StudentMainFragment"
        android:label="StudentMainFragment"
        tools:layout="@layout/student_main_fragment">

        <action
            android:id="@+id/action_studentMainFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/graph_nav"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_studentMainFragment_to_scheduleFragment"
            app:destination="@id/scheduleFragment" />

        <action
            android:id="@+id/action_studentMainFragment_to_performanceFragment"
            app:destination="@id/performanceFragment" />

        <action
            android:id="@+id/action_studentMainFragment_to_deadlineFragment"
            app:destination="@id/deadlineFragment" />
        <action
            android:id="@+id/action_studentMainFragment_to_locationHistoryFragment"
            app:destination="@id/locationHistoryFragment" />

        <action
            android:id="@+id/action_studentMainFragment_to_attendanceFragment"
            app:destination="@id/attendanceFragment" />
        <action
            android:id="@+id/action_studentMainFragment_to_examTableFragment"
            app:destination="@id/examTableFragment" />
        <action
            android:id="@+id/action_studentMainFragment_to_referenceFragment"
            app:destination="@id/referenceFragment" />
    </fragment>
    <fragment
        android:id="@+id/locationHistoryFragment"
        android:name="uz.jbnuu.tsc.parents.ui.location_history.LocationHistoryFragment"
        android:label="LocationHistoryFragment"
        tools:layout="@layout/location_history_fragment">
        <action
            android:id="@+id/action_locationHistoryFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/graph_nav"
            app:popUpToInclusive="true" />
    </fragment>

    <fragment
        android:id="@+id/splashFragment"
        android:name="uz.jbnuu.tsc.parents.ui.splash.SplashFragment"
        android:label="SplashFragment"
        tools:layout="@layout/fragment_splash">
        <action
            android:id="@+id/action_splashFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/graph_nav"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_splashFragment_to_adminFragment"
            app:destination="@id/adminFragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_splashFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true" />


    </fragment>
    <fragment
        android:id="@+id/scheduleFragment"
        android:name="uz.jbnuu.tsc.parents.ui.timetable.ScheduleFragment"
        android:label="SchedukeFragment"
        tools:layout="@layout/schedule_fragment" />
    <fragment
        android:id="@+id/attendanceFragment"
        android:name="uz.jbnuu.tsc.parents.ui.attendance.AttendanceFragment"
        android:label="AttendanceFragment"
        tools:layout="@layout/attendance_fragment" />
    <fragment
        android:id="@+id/performanceFragment"
        android:name="uz.jbnuu.tsc.parents.ui.performance.PerformanceFragment"
        android:label="PerformanceFragment"
        tools:layout="@layout/performance_fragment" />
    <fragment
        android:id="@+id/adminFragment"
        android:name="uz.jbnuu.tsc.parents.base.AdminFragment"
        android:label="AdminFragment"
        tools:layout="@layout/admin_fragment" />
    <fragment
        android:id="@+id/deadlineFragment"
        android:name="uz.jbnuu.tsc.parents.ui.deadlines.DeadlineFragment"
        android:label="DeadlineFragment"
        tools:layout="@layout/deadlines_fragment" />


    <fragment
        android:id="@+id/examTableFragment"
        android:name="uz.jbnuu.tsc.parents.ui.examTable.ExamTableFragment"
        android:label="ExamTableFragment"
        tools:layout="@layout/exam_table_fragment" />
    <fragment
        android:id="@+id/referenceFragment"
        android:name="uz.jbnuu.tsc.parents.ui.reference.ReferenceFragment"
        android:label="ReferenceFragment"
        tools:layout="@layout/reference_fragment" />
    <fragment
        android:id="@+id/registerFragment"
        android:name="uz.jbnuu.tsc.parents.ui.register.RegisterFragment"
        android:label="RegisterFragment"
        tools:layout="@layout/register_fragment" />
    <fragment
        android:id="@+id/mainFragment"
        android:name="uz.jbnuu.tsc.parents.ui.main.MainFragment"
        android:label="MainFragment"
        tools:layout="@layout/main_fragment">
        <action
            android:id="@+id/action_mainFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/graph_nav"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_mainFragment_to_studentMainFragment"
            app:destination="@id/studentMainFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_mainFragment_to_settingsFragment"
            app:destination="@id/settingsFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="uz.jbnuu.tsc.parents.ui.settings.SettingsFragment"
        android:label="SettingsFragment"
        tools:layout="@layout/settings_fragment">
        <action
            android:id="@+id/action_settingsFragment_to_personalFragment"
            app:destination="@id/personalFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_settingsFragment_to_removeStudentFragment"
            app:destination="@id/removeStudentFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/personalFragment"
        android:name="uz.jbnuu.tsc.parents.ui.settings.personal.PersonalFragment"
        android:label="PersonalFragment"
        tools:layout="@layout/personal_fragment" />
    <fragment
        android:id="@+id/removeStudentFragment"
        android:name="uz.jbnuu.tsc.parents.ui.remove_student.RemoveStudentFragment"
        android:label="RemoveStudentFragment"
        tools:layout="@layout/remove_student_fragment" />
</navigation>